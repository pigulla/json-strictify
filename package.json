{
  "name": "json-strictify",
  "version": "6.3.3",
  "description": "Safely serialize a value to JSON without unintended loss of data or going into an infinite loop due to circular references.",
  "engines": {
    "node": ">=10"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*",
    "CHANGELOG.md",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm ci && rm -rf dist/ && npm test && tsc --build tsconfig.json",
    "test": "tsc --noEmit && npm run lint && npm run test-with-coverage && npm audit && npm run lockfile-lint",
    "lint": "eslint --ext=.ts 'src/**/*.ts' 'test/**/*.ts'",
    "lockfile-lint": "npx lockfile-lint --path package-lock.json --type npm --validate-https --allowed-hosts npm",
    "mocha": "mocha",
    "test-with-coverage": "nyc mocha 'test/**/*.spec.ts'",
    "coveralls": "coveralls < coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pigulla/json-strictify.git"
  },
  "keywords": [
    "JSON",
    "debugging",
    "validation",
    "stringify",
    "strict"
  ],
  "author": "Raphael Pigulla <pigulla@four66.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pigulla/json-strictify/issues"
  },
  "homepage": "https://github.com/pigulla/json-strictify",
  "dependencies": {
    "json-pointer": "^0.6.0",
    "ts-custom-error": "^3.1.1",
    "tslib": "^1.11.1"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.2.11",
    "@types/json-pointer": "^1.0.30",
    "@types/lodash.noop": "^3.0.6",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.0",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.11",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "lodash.noop": "^3.0.1",
    "minimist": "^1.2.5",
    "mocha": "^7.1.1",
    "nyc": "^15.0.0",
    "ts-node": "^8.8.1",
    "tsconfig-paths": "^3.9.0",
    "type-fest": "^0.13.0",
    "typescript": "^3.8.3"
  },
  "resolutions": {
    "minimist": "^1.2.5"
  }
}
