{
  "name": "json-strictify",
  "version": "8.0.0",
  "description": "Safely serialize a value to JSON without unintended loss of data or going into an infinite loop due to circular references.",
  "engines": {
    "node": ">=14"
  },
  "main": "./dist/index.js",
  "type": "module",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*",
    "CHANGELOG.md",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm ci && rm -rf dist/ && npm test && tsc --build tsconfig.json",
    "test": "tsc --noEmit && npm run lint && npm run jest -- --coverage && npm run audit && npm run lockfile-lint",
    "lint": "eslint --ext=.ts .",
    "audit": "npm audit --audit-level high",
    "lockfile-lint": "npx lockfile-lint --path package-lock.json --type npm --validate-https --allowed-hosts npm",
    "jest": "jest",
    "coveralls": "coveralls < coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pigulla/json-strictify.git"
  },
  "keywords": [
    "JSON",
    "debugging",
    "validation",
    "stringify",
    "strict"
  ],
  "author": "Raphael Pigulla <pigulla@four66.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pigulla/json-strictify/issues"
  },
  "homepage": "https://github.com/pigulla/json-strictify",
  "dependencies": {
    "json-pointer": "^0.6.1",
    "ts-custom-error": "^3.2.0",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.21",
    "@types/jest": "^26.0.24",
    "@types/json-pointer": "^1.0.30",
    "@types/lodash.noop": "^3.0.6",
    "@types/node": "^14.17.9",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "chai": "^4.3.4",
    "coveralls": "^3.1.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.2.0",
    "type-fest": "^2.0.0",
    "typescript": "^4.3.5"
  }
}
