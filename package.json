{
    "name": "json-strictify",
    "version": "6.2.0",
    "description": "Safely serialize a value to JSON without unintended loss of data or going into an infinite loop due to circular references.",
    "engines": {
        "node": ">=10"
    },
    "main": "./dist/JSONs.js",
    "types": "./index.d.ts",
    "files": [
        "index.d.ts",
        "dist/**/*",
        "CHANGELOG.md",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "build": "rm -rf dist/ && rm -f index.d.ts && tsc --build tsconfig.json && npm run ts-definitions",
        "ts-definitions": "tsc --declaration --emitDeclarationOnly --outFile index.d.ts",
        "test": "npm run lint && npm run build && npm run test-with-coverage && npm run check-coverage && npm audit",
        "lint": "eslint --ext=.ts .",
        "mocha": "mocha 'test/**/*.spec.js'",
        "test-with-coverage": "nyc mocha",
        "check-coverage": "nyc check-coverage",
        "coveralls": "coveralls < coverage/lcov.info"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/pigulla/json-strictify.git"
    },
    "keywords": [
        "JSON",
        "debugging",
        "validation",
        "stringify",
        "strict"
    ],
    "author": "Raphael Pigulla <pigulla@four66.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/pigulla/json-strictify/issues"
    },
    "homepage": "https://github.com/pigulla/json-strictify",
    "dependencies": {
        "json-pointer": "^0.6.0",
        "ts-custom-error": "^3.1.1"
    },
    "devDependencies": {
        "@types/json-pointer": "^1.0.30",
        "@types/node": "^12.12.7",
        "@typescript-eslint/eslint-plugin": "^2.6.1",
        "@typescript-eslint/parser": "^2.6.1",
        "chai": "^4.2.0",
        "coveralls": "^3.0.7",
        "eslint": "^6.6.0",
        "eslint-config-standard": "^14.1.0",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-node": "^10.0.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-standard": "^4.0.1",
        "jest": "^24.9.0",
        "lodash.noop": "^3.0.1",
        "mocha": "^6.2.2",
        "nyc": "^14.1.1",
        "rewire": "^4.0.1",
        "typescript": "^3.7.2"
    }
}
